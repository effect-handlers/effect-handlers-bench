import list
import option
import args
import string

def fibonacci(n: Int): Int =
  if (n == 0) {
    0
  } else if (n == 1) {
    1
  } else {
    fibonacci(n - 1) + fibonacci(n - 2)
  }

def main() = {
  val n = on[WrongFormat].default{5}{
    commandLineArgs().headOption.getOrElse{""}.toInt
  }
  val r = fibonacci(n);
  println(r)
}