import list
import option
import args
import string

effect Emit(e: Int): Unit

def range(l: Int, u: Int): Unit / Emit =
  if (l > u) {
    ()
  } else {
    do Emit(l);
    range(l + 1, u)
  }

def run(n: Int) = {
  var s = 0;
  try {
    range(0, n);
    s
  } with Emit { (e: Int) =>
    s = s + e;
    resume(())
  }
}

def main() = {
  val n = on[WrongFormat].default{5}{
    commandLineArgs().headOption.getOrElse{""}.toInt
  }
  val r = run(n);
  println(r)
}
